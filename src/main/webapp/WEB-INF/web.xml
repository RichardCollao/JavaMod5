<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>JM5AE1G</display-name>

	<servlet>
		<servlet-name>Inicio</servlet-name>
		<servlet-class>com.company.servlets.Inicio</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Contacto</servlet-name>
		<servlet-class>com.company.servlets.Contacto</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Capacitacion</servlet-name>
		<servlet-class>com.company.servlets.Capacitacion</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ListarCapacitaciones</servlet-name>
		<servlet-class>com.company.servlets.ListarCapacitaciones</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ListarUsuarios</servlet-name>
		<servlet-class>com.company.servlets.ListarUsuarios</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.company.servlets.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Test</servlet-name>
		<servlet-class>com.company.servlets.Test</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Inicio</servlet-name>
		<url-pattern>/inicio</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Contacto</servlet-name>
		<url-pattern>/contacto</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Capacitacion</servlet-name>
		<url-pattern>/capacitacion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListarCapacitaciones</servlet-name>
		<url-pattern>/listarcapacitaciones</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListarUsuarios</servlet-name>
		<url-pattern>/listarusuarios</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
</web-app>